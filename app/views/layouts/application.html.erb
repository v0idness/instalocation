<!DOCTYPE html>
<html>
<head>
  <title>Tweeporter</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<div class="header">
<ul class="navigation">
	<li id="logo"><a href="#"><img src="#" alt="Tweeporter Logo"></a></li>
	<li id="create"><a href="#"><img src="#" alt="add new event"></a></li>
	<li><a href="#">North America</a></li>
	<li><a href="#">Europe</a></li>
	<li><a href="#">Asia</a></li>
</ul>
<p>
search bar
</p>
<p>
<div id="user_nav">
	<% if user_signed_in? %>
	Signed in as <%= current_user.email %> <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
	<% else %>
		<%= link_to "Sign up", new_user_registration_path %> or <%= link_to "Sign in", new_user_session_path %>
	<% end %>
</div>		 
</p>
</div>
<div style="clear:both;">

<%= yield %>
</div>
</body>
</html>

